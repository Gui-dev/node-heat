// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database/dev.db"
}

model User {
  id         String @id @default(uuid())
  name       String
  github_id  Int
  login      String
  avatar_url String

  @@map("users")
}
